<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ir_cron_request_subdomain_stop_time" model="ir.cron">
        <field name="name">Subdomain: Stop Service by Stop Time</field>
        <field name="model_id" ref="model_request_subdomain_requestsubdomain"/>
        <field name="state">code</field>
        <field name="code">model.corn_check_stop_time()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
        <field name="priority">1000</field>
        <field name="active">True</field>
    </record>


</odoo>